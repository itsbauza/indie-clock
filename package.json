{
  "name": "indie-clock",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "dotenv -e .env.local prisma generate",
    "db:push": "dotenv -e .env.local prisma db push",
    "db:migrate": "dotenv -e .env.local prisma migrate dev",
    "db:studio": "dotenv -e .env.local prisma studio",
    "db:seed": "dotenv -e .env.local tsx prisma/seed.ts",
    "rabbitmq:setup": "dotenv -e .env.local tsx scripts/setup-rabbitmq.ts",
    "rabbitmq:monitor": "node scripts/monitor-queue.js",
    "rabbitmq:messages": "dotenv -e .env.local node scripts/view-messages.js",
    "rabbitmq:cleanup": "dotenv -e .env.local node scripts/cleanup-messages.js",
    "rabbitmq:test": "node scripts/test-message.js",
    "mqtt:monitor": "node scripts/monitor-mqtt.js",
    "mqtt:test": "node scripts/test-mqtt.js",
    "mqtt:auth": "node scripts/test-mqtt-auth.js",
    "mqtt:test-credentials": "node scripts/test-device-credentials.js",
    "mqtt:fix-permissions": "node scripts/fix-mqtt-permissions.js",
    "mqtt:view": "node scripts/view-messages.js",
    "devices:check": "node scripts/check-devices.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:health": "node scripts/docker-health-check.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/adapter-pg": "^6.10.1",
    "@prisma/client": "^5.10.0",
    "@types/mqtt": "^0.0.34",
    "@types/pg": "^8.15.4",
    "amqplib": "^0.10.3",
    "mqtt": "^5.13.1",
    "next": "15.3.4",
    "next-auth": "^5.0.0-beta.29",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/amqplib": "^0.10.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "prisma": "^5.10.0",
    "tailwindcss": "^4",
    "tsx": "^4.7.0",
    "typescript": "^5"
  }
}
