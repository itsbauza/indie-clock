{
  "users": [
    {
      "name": "${RABBITMQ_ADMIN_USER}",
      "password": "${RABBITMQ_ADMIN_PASSWORD}",
      "tags": "administrator"
    },
    {
      "name": "${RABBITMQ_BACKEND_USER}",
      "password": "${RABBITMQ_BACKEND_PASSWORD}",
      "tags": "management"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "${RABBITMQ_ADMIN_USER}",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    },
    {
      "user": "${RABBITMQ_BACKEND_USER}",
      "vhost": "/",
      "configure": "amq\\.topic|users\\..*|awtrix.*|mqtt-subscription-.*",
      "write": "amq\\.topic|users\\..*|awtrix.*|mqtt-subscription-.*",
      "read": "amq\\.topic|users\\..*|awtrix.*|mqtt-subscription-.*"
    }
  ],
  "exchanges": [
    {
      "name": "amq.topic",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "queues": [],
  "bindings": [],
  "policies": [
    {
      "vhost": "/",
      "name": "mqtt-messages-ttl",
      "pattern": "^(users|awtrix)\\.",
      "apply-to": "queues",
      "definition": {
        "message-ttl": 1800000,
        "max-length": 500,
        "max-length-bytes": 10485760,
        "overflow": "drop-head",
        "expires": 7200000
      },
      "priority": 0
    },
    {
      "vhost": "/",
      "name": "memory-optimization",
      "pattern": ".*",
      "apply-to": "queues",
      "definition": {
        "max-length": 1000,
        "max-length-bytes": 52428800,
        "overflow": "drop-head"
      },
      "priority": 1
    }
  ]
} 